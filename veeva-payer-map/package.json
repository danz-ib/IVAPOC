  "license": "MIT",
  "devDependencies": {
    "live-server": "^1.2.1",
    "html5validator": "^0.4.2"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/company/zenpep-payer-map.git"
  },
  "bugs": {
    "url": "https://github.com/company/zenpep-payer-map/issues"
